{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "func-names": "off",
    "consistent-return": "off",
    "no-underscore-dangle": "off",
    "comma-dangle": ["error", "never"],
    "max-len": ["error", { "code": 200 }]
  }
}
